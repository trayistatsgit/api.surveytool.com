{
  "name": "api.surveytool.com",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "build:sandbox": "env-cmd -f .env.sandbox npm run build-ts",
    "build:production": "env-cmd -f .env.production npm run build-ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|json)\"",
    "serve": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "geoip-lite": "^1.4.10",
    "morgan": "^1.10.0",
    "mssql": "^9.3.2",
    "multer": "^1.4.5-lts.1",
    "sequelize": "^6.37.4",
    "tedious": "^18.6.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/morgan": "^1.9.9",
    "@types/mssql": "^9.1.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
